<%- include('../../partials/navbar.ejs')-%>

<body>
    <div class="container">
        <div class="card mt-3">
             <div class="card-header">
                <h2>Cadastro de Tipos</h2>
             </div>
             <div class="card-body">
                <form  action="/tipos/cadastrar" method="post">

                    <label for="codigo">Código</label>
                    <input class="form-control mt-3" type="text" name="codigo" id="codigo">
                    <label for="descricao">Descrição</label>
                    <input class="form-control mt-3" type="text" name="descricao" id="descricao">
                    <button class="btn btn-success mt-3" type="submit">Cadastrar</button>
                    <button class="btn btn-primary mt-3" type="reset">Limpar</button>
                </form>
             </div>
        </div>



    </div>
</body>


